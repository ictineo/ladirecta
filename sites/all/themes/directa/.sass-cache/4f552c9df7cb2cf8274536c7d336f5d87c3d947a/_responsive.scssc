:@template"|2/**
 * @file
 * Positioning for a responsive layout.
 *
 * Define CSS classes to create a fluid grid layout with optional sidebars
 * depending on whether blocks are placed in the left or right sidebars.
 *
 * This layout uses the Zen Grids plugin for Compass: http://zengrids.com
 */

// We are going to create a fluid grid with 1, 3, or 5 columns and 20px gutters
// between columns (applied as 10px of left/right padding on each column).
$zen-column-count:  1;
$zen-gutter-width:  20px;

// If you need IE6/7 support for box-sizing: border-box (default), see _base.scss
//$zen-box-sizing: content-box;

// You can generate more efficient CSS if you manually apply the
// zen-grid-item-base mixin to all grid items from within a single ruleset.
$zen-auto-include-item-base: false;
// $zen-auto-include-flow-item-base: false;

// Suppress this section of CSS for RTL layouts since it contains no LTR-specific styles.
@if $zen-reverse-all-floats == false {

/**
 * Center the page.
 *
 * For screen sizes larger than 1200px, prevent excessively long lines of text
 * by setting a max-width.
 */
#page,
.region-bottom {
  margin-left: auto;
  margin-right: auto;
  max-width: 1280px;
}

/* Apply the shared properties of grid items in a single, efficient ruleset. */
#header,
#content,
#navigation,
.region-sidebar-first,
.region-sidebar-second,
#footer {
  // See the note about $zen-auto-include-item-base above.
  @include zen-grid-item-base();
}

/* Containers for grid items and flow items. */
#header,
#main,
#footer {
  @include zen-grid-container();
}

/**All**/
#name-and-slogan {
  display: none;
}



/* Navigation bar */
@media all and (min-width: 480px) {
  #main {
    /* Move all the children of #main down to make room. */
    //padding-top: $nav-height;
    position: relative;
  }
  #navigation {
    /* Move the navbar up inside #main's padding. */
    //position: absolute;
    top: 0;
    //height: $nav-height;
    width: $zen-grid-width;
  }
}

} // End of @if $zen-reverse-all-floats == true

/**
 * Use 3 grid columns for smaller screens.
 */
@media all and (min-width: 480px) and (max-width: 959px) {

  $zen-column-count: 3;

  /**
   * The layout when there is only one sidebar, the left one.
   */
  .sidebar-first {
    /* Span 2 columns, starting in 2nd column from left. */
    #content {
      @include zen-grid-item(2, 2);
    }

    /* Span 1 column, starting in 1st column from left. */
    .region-sidebar-first {
      @include zen-grid-item(1, 1);
    }
  }

  /**
   * The layout when there is only one sidebar, the right one.
   */
  .sidebar-second {
    /* Span 2 columns, starting in 1st column from left. */
    #content {
      @include zen-grid-item(2, 1);
    }

    /* Span 1 column, starting in 3rd column from left. */
    .region-sidebar-second {
      @include zen-grid-item(1, 3);
    }
  }

  /**
   * The layout when there are two sidebars.
   */
  .two-sidebars {
    /* Span 2 columns, starting in 2nd column from left. */
    #content {
      @include zen-grid-item(2, 2);
    }

    /* Span 1 column, starting in 1st column from left. */
    .region-sidebar-first {
      @include zen-grid-item(1, 1);
    }

    /* Start a new row and span all 3 columns. */
    .region-sidebar-second {
      @include zen-grid-item(3, 1);
      @include zen-nested-container(); // Since we're making every block in this region be a grid item.
      @include zen-clear();

      /* Apply the shared properties of grid items in a single, efficient ruleset. */
      .block {
        @include zen-grid-item-base();
      }
      /* Span 1 column, starting in the 1st column from left. */
      .block:nth-child(3n+1) {
        @include zen-grid-item(1, 1);
        @include zen-clear();
      }
      /* Span 1 column, starting in the 2nd column from left. */
      .block:nth-child(3n+2) {
        @include zen-grid-item(1, 2);
      }
      /* Span 1 column, starting in the 3rd column from left. */
      .block:nth-child(3n) {
        @include zen-grid-item(1, 3);
      }
    }
  }
}

/**
 * Use 5 grid columns for larger screens.
 */
@media all and (min-width: 960px) {
  $zen-column-count: 23;
  $zen-gutter-width:20px;
  #header {
    //@include zen-grid-item(23,1);
    //@include zen-nested-container();
    //@include zen-grid-container();
    #logo {
      @include zen-grid-item(6,1);
    }
    #name-and-slogan {
      display:none;
    }
    #secondary-menu {
      //@include zen-grid-item(2,10);
    }
    .region-header  {
      @include zen-clear(none);
      @include zen-grid-item(17,1,right);
      $zen-column-count:18;
      $zen-gutter-width:20px;
      @include zen-grid-container();
      #block-block-4 {
        @include zen-grid-item(2,2);
      }
      #block-block-5 {
        //Subscriu't-'hi
        @include zen-grid-item(3,6);
        display:none;
      }
      #block-block-1 {
        @include zen-grid-item(4,9);
      }
      #block-block-3 {
        //Inicia sessio
        @include zen-grid-item(3,6);
      }
      #block-views-exp-hemeroteca-page-1 {
        @include zen-grid-item(6,13);
        @include zen-nested-container();
        #edit-search-api-views-fulltext-wrapper {
          @include zen-grid-item(17,1);
        }
        .views-submit-button {
          display:none;
        }
      }
      #block-block-2 {
        @include zen-grid-item(2,4); /*asignando ancho del bloque xxss*/
        @include zen-nested-container(); /*indico que se subdivide, crea una graella igual a la heredada de 23*/
        .tw {
        @include zen-grid-item(7,2); /*distribuyo cada elemento tomando de referencia 23*/
        }
        .fb {
        @include zen-grid-item(7,8);
        }
        .vi {
        @include zen-grid-item(7,14);
        }
      }
      #block-system-user-menu {
        @include zen-grid-item (3,6);
      }
    }
  }


  #navigation .region-navigation {
    @include zen-clear(both);
    $zen-column-count:23;
    $zen-gutter-width:10px;
    @include zen-grid-container();
    #block-nice-menus-3 ul {
      @include zen-grid-item(7,1);
      $zen-column-count:2;
      @include zen-grid-container();
      .odd {
        @include zen-grid-item(1,1);
        @include zen-clear(left);
      }
      .even {
        @include zen-grid-item (1,2);
      }
     }
     #block-nice-menus-2 ul {
      $zen-column-count:23;
      $zen-gutter-width:10px;
      @include zen-grid-item(7,8);
      $zen-column-count:2;
         @include zen-grid-container();
         .odd {
          @include zen-grid-item(1,1);
          @include zen-clear(left);
          }
         .even {
          @include zen-grid-item (1,2);
          }  
     }
     
     #block-nice-menus-1 ul {
       $zen-column-count:23;
       $zen-gutter-width:10px;
       @include zen-grid-item(6,1,right);
       $zen-column-count:1;
       @include zen-grid-container();
      li {
        @include zen-grid-item(1,1,right);
        @include zen-clear(right);
      }
    }
  }


  #content,
  .sidebars,
  .region-help {
  }
  .region-help {
    display:none;
  }
 .not-front {
   #main {
     $zen-column-count:23;
     $zen-gutter-width:10px; 
     @include zen-grid-container();
     #content {
       @include zen-grid-item(17,1);
     }
     .sidebars .region-sidebar-first {
       @include zen-grid-item(6,18);
       display: none;
     }
     .sidebars .region-sidebar-second {
       @include zen-grid-item(6,18);
     }
   }
 }

 .page-fotogaleria {
   #main {
     $zen-column-count:23;
     $zen-gutter-width:10px; 
     @include zen-grid-container();
     #content {
       @include zen-grid-item(23,1);
     }
     .sidebars .region-sidebar-first {
       @include zen-grid-item(6,18);
       display: none;
     }
     .sidebars .region-sidebar-second {
       @include zen-grid-item(6,18);
       display: none;
     }
   }
 }


 .front {
   #main {
     $zen-column-count:23;
     $zen-gutter-width:10px; 
     @include zen-grid-container();
     #content {
       @include zen-grid-item(9,1);
     }
     .sidebars .region-sidebar-first {
       @include zen-grid-item(8,10);
     }
     .sidebars .region-sidebar-second {
       @include zen-grid-item(6,18);
     }
     #destacat {
       @include zen-clear(both);
       @include zen-nested-container();
       .field-fotografies,
       .field-portada-fotogaleria {
         @include zen-grid-item(11,1);
       }
       .views-field-nothing {
         @include zen-grid-item(12,1,right);
       }
       .views-field-title {
         @include zen-clear(right);
         @include zen-grid-item(12,1,right);
       }
       .field-body,
       .field-continguts-relacionats-ref,
       .field-continguts-relacionats {
         @include zen-clear(right);
         @include zen-grid-item(12,1,right);
       }
       .attachment {
         @include zen-clear(both);
         $zen-column-count:4;
         $zen-gutter-width:10px;
         @include zen-grid-container();
         .field-fotografies {
           @include zen-grid-item(2,1);
         }
         .autor,
         .date-display-single,
         .field-body {
           @include zen-clear(right);
           @include zen-grid-item(2,3);
         }
       }
     }
     
   } 
  
 }

 /**FOOTER**/

 #footer {
   @include zen-clear(both);
   $zen-column-count:10;
   $zen-gutter-width:10px;
   @include zen-grid-container();
   #block-menu-menu-menu-footer {
     @include zen-grid-item(7,1);
   }
   #block-block-10 {
     @include zen-grid-item(3,1,right);
     @include zen-clear(right);
   }
   #views-exposed-form-hemeroteca-page-1,
   #block-views-exp-hemeroteca-page-1--2 {
     @include zen-grid-item(3,1,right);
     @include zen-clear(right);
     .views-exposed-form {
       float:right;
     }
   }
   #block-block-9 {
     @include zen-clear(right);
     @include zen-grid-item(2,1,right);
   }
   #block-menu-menu-menu-xxss {
     @include zen-grid-item(1,7);
   }

   #block-nice-menus-2,
   #block-nice-menus-2--2 {
     @include zen-clear();
     @include zen-grid-item(3,1);
   }
   #block-nice-menus-3,
   #block-nice-menus-3--2 {
     @include zen-grid-item(3,4);
    }

   #block-nice-menus-2,
   #block-nice-menus-2--2 {
      $zen-column-count:3;
       $zen-gutter-width:10px;
       @include zen-grid-container();
       li.odd {
         @include zen-grid-item(3,1);
         @include zen-clear(left);
       }
       li.even {
         @include zen-clear();
         @include zen-grid-item(3,4);
         @include zen-clear(right);
       }
   }
   #block-nice-menus-3,
   #block-nice-menus-3--2 {
     $zen-column-count:3;
     $zen-gutter-width:10px;
     @include zen-grid-container();
     li.odd {
       @include zen-grid-item(3,1);
       @include zen-clear(left);
     }
     li.even {
       @include zen-grid-item(2,4);
       @include zen-clear(right);
     }
     
   }
 }

/**LOGIN**/

.not-logged-in.page-user {
   #header,
   #navigation,
   .breadcrumb,
   #footer {
     display: none;
   }
   #main {
     $zen-column-count:23;
     $zen-gutter-width:10px;
     @include zen-grid-container();

   #content {
     @include zen-grid-item(23,1);
     $zen-column-count:4;
     $zen-gutter-width:40px;
     @include zen-grid-container();
     @include zen-grid-background();
     h1#page-title {
       @include zen-grid-item(2,2);
       @include zen-clear(both);
     }
     ul li.tabs-primary__tab.is-active {
       @include zen-clear(both);
       @include zen-grid-item(1,2);
       display: none;
     }
     ul li.tabs-primary__tab {
       @include zen-clear(both);
       @include zen-grid-item(1,3);
       display: none;
     }
     #edit-account,
     #user-pass,
     #user-login {
       @include zen-clear();
       @include zen-grid-item(1,2,left);
     }
     #block-block-5--2 {
       @include zen-grid-item(1,2,right);
       @include zen-clear(left);
     }
     #block-block-6 {
       @include zen-clear();
       @include zen-grid-item(1,3);
     }
     #block-block-8 {
       @include zen-grid-item(1,2);
     }
   }
 }
   .region-sidebar-first {
     $zen-column-count:4;
     $zen-gutter-width:40px;
     @include zen-grid-container();
     @include zen-grid-background();
     #block-block-8 {
       @include zen-grid-item(1,2);
       }
     }
 }

/*NOTICIA COMPLETA**/

 .node-type-noticia {
   .breadcrumb {
     display: none;
   }
 }






