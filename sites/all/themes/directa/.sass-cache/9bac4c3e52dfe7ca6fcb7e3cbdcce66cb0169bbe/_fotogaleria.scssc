/**
 * mixin de comportament del block de 'totes les fotogaleries'
 * tant pel llistat com per la vista del node
 */
@mixin block-totes-fotogaleries() {
  .view-content {
    height: 100px;
    overflow: hidden;
  }
  .view-footer {
    cursor: pointer;
  }
}



/*****
 * OJU DOS ARXIUS AJUNTATS, ES PODEN REPETIR
 * DEFINICIONS
 * REVISAR
 */
.page-fotogaleria,
.page-videogaleria,
.not-front.node-type-fotogaleria, 
.not-front.node-type-video {
  #content {
    margin-top: 30px;
    $zen-column-count:23;   
    $zen-gutter-width:10px;    
    @include zen-grid-container();
    #page-title {
      //.meta-info-top { //camps de node
      @include zen-clear();
      @include zen-grid-item(21,1);
    }
    .meta-info-top {
      @include zen-grid-item(8,1);
    }
    //.field-seccio-web {
      //@include zen-grid-item(2,1);
    //}
    .node-fotogaleria,
    .node-video,
    .view-videogaleria, 
    .view-fotogaleria {
      @include zen-clear();
      @include zen-grid-item(23,1);
      .field-video {  //pels camps de node  
        @include zen-clear();
        @include zen-grid-item(21,1);
      }
      .field-body {
        @include zen-clear();
        @include zen-grid-item(17,4);
      }
      .easy_social_box {
        @include zen-grid-item(3,1);
      }
      fieldset.group-tags {
        @include zen-clear();
        @include zen-grid-item(17,4);
      }
      .field-fotografies  {
        @include zen-clear();
        //@include zen-grid-item(23,1);
        .field-slideshow {
          @include zen-grid-item(19,3);
        }
        .field-slideshow-caption {
          @include public-roman(21px, $gris7,30px);
        }
        #field-slideshow-1-controls {
          a.prev {
            @include zen-grid-item(1,1);
          }
          a.next {
            @include zen-grid-item(1,21);
          }
        }
        .item-list {
          @include zen-clear();
          @include zen-grid-item(20,2);
        }
      }

      .views-row { //pels camps de view
        #top-wr {
          @include zen-grid-item (8,1);
        }
        .field-seccio-web {
          //@include zen-grid-item(2,1);
          display: inline-block;
          float: left;
          margin-right: 10px;
          background-image: url('../images/pattern-gris.png');
          }
          .field-autor {
            //@include zen-grid-item(5,3);
            display: inline-block;
            float: left;
            font-size: 12px;
            line-height: 18px;
            color: $gris3;
          }
        .views-field-title {
          @include zen-clear();
          @include zen-grid-item(21,1);
        }
        .field-data {
          display:inline;
        }
        .field-video{
          @include zen-grid-item(21,1);
          @include zen-clear();
        }
        .field-fotografies  {
          @include zen-clear();
          @include zen-grid-item(23,1);
          .field-slideshow {
            @include zen-grid-item(19,3);
          }
          .field-slideshow-caption {
            @include public-roman(21px, $gris7,30px);
          }
          #field-slideshow-1-controls {
            a.prev {
              @include zen-grid-item(1,1);
            }
            a.next {
              @include zen-grid-item(1,21);
            }
          }

          .item-list {
            @include zen-clear();
            @include zen-grid-item(20,2);
          }
        }
        .easy_social_box {
          @include zen-grid-item(3,1);
        }
        #tags-wrapper {
          @include zen-clear();
          @include zen-grid-item(17,4);
        }
        .field-body { 
          @include zen-clear();
          @include zen-grid-item(17,4);
        }
        .flag-outer {
          display:none;
        }
        //.field-etiquetes {
          //@include zen-clear(both);
          //@include zen-grid-item(8,2);
          //@include etiquetes-2;
          //margin-bottom:100px;
        //}
      }
    }
    $zen-column-count: 23;
    @include zen-grid-container();
    #block-views-fotogaleria-block,
    #block-views-videogaleria-block {
      @include zen-clear();
      @include zen-grid-item(17,1);
      $zen-column-count: 3;
      $zen-gutter-width: 10px;
      border-right: 1px solid $fons;
      padding-right:20px;
      @include zen-grid-container();
      h2.block-title {
        @include zen-grid-item(3,1);
        margin-bottom:10px;
      }
      td.col-1 {
        @include zen-grid-item(1,1);
      }
      td.col-2 {
        @include zen-grid-item(1,2);
      }
      td.col-3 {
        @include zen-grid-item(1,3);
      }
      ul {
        list-style: none;
        padding-left: 0;
        $zen-column-count: 3;
        $zen-gutter-width: 10px;
        @include zen-grid-container();
        li {
          padding-bottom: 10px;
        }
        li.views-row-first {
          display: none;
        }
        li:nth-child(3n) {
          @include zen-grid-item(1,3);
        }
        li:nth-child(3n+1) {
          @include zen-grid-item(1,1);

        }
        li:nth-child(3n+2) {
          @include zen-grid-item(1,2);
          @include zen-clear();
        }
      }
    }
    .view-id-fotogaleria.view-display-id-page,
    .view-id-videogaleria.view-display-id-page {
      .field-slideshow-pager.slides-4 {
        @include zen-clear();
      }
    }
    $zen-column-count:23;
    #block-directa-custom-openx-banner-lateralpetit4,
    #block-directa-custom-openx-banner-lateralpetit3,
    #block-directa-custom-openx-banner-lateralpetit2,
    #block-directa-custom-openx-banner-lateralgran1,
    #block-directa-custom-openx-banner-lateralpetit1 {
      @include zen-grid-item(6,18);
    }
  }



/*ESTILS*/

    .view-id-fotogaleria.view-display-id-page,
    .view-id-videogaleria.view-display-id-page {
      margin-top: -20px;
      .views-field-title.field-content {
        @include graphik-semibold(18px, $gris7,22px); /*variacio disseny*/
      }  
      .field-fotografies {
        .field-slideshow-caption {
          @include public-roman(21px,$gris7,30px);
        }
        .field-slideshow-controls {
          a.prev {
            background-image: url('../images/prev.png');
            background-position: 38px 0;
            background-repeat: no-repeat;
            margin-top: 200px;
            height: 30px;
          }
          a.next {
            background-image: url('../images/next.png');
            background-position: 38px 0;
            background-repeat: no-repeat;
            margin-top: 200px;
            height: 30px;
          }
        }
        .item-list ul {
          list-style: none;
        }
      }
    }
    .node-fotogaleria {
      .field-slideshow-caption {
        @include public-roman(21px,$gris7,30px);
      }
      .field-slideshow-controls {
        a.prev {
          background-image: url('../images/prev.png');
          background-position: 38px 0;
          background-repeat: no-repeat;
          margin-top: 200px;
          height: 30px;
        }
        a.next {
          background-image: url('../images/next.png');
          background-position: 38px 0;
          background-repeat: no-repeat;
          margin-top: 200px;
          height: 30px;
        }
      }
      .item-list ul {
        list-style: none;
      }
    }

    #content {
      #page-title, .page-title , .views-field-title, .views-field-title a {
        @include titulo-fotogaleria;
        margin-top: 10px;
        margin-bottom:10px;
        display: block;
        font-weight: bolder;
        font-weight: bolder;

    }
  }
  #content {
    .views-field-title,
    .views-field-title a {
      margin-top: 0px;
    }
  }
  fieldset.group-top-wr {
    border: none 0px;
    legend {
      display: none;
    }
  }
  .meta-info-top {
    .field-seccio-web {
      display: inline-block;
      background-image: url('../images/pattern-gris.png');
    }
    .autories {
      .field-autor,
      .field-data {
        display: inline-block;
      }
    }
  }
  .field-autor,
  .field-autor a {
    /** altura de tota la caixa i centrem amb middle**/
    @include graphik-medium(12px,$gris3,35px);
    vertical-align: middle;
    text-transform: uppercase;
    text-decoration: underline;   
  }
  .date-display-single {
    &:before {
      display: inline-block;
      content: '/ ';
    }
    @include graphik-medium(12px,$gris3,35px);
    vertical-align: middle;
  }
  .field-body {
    @include public-roman(23px,$gris7,32px);
    p.first-of-type {
      margin-top: 0;
    }
  }
  .views-field-easy-social, 
  .easy_social_box {
    margin-top: 50px;
  }
  #tags-wrapper,
  fieldset.group-tags {
    legend {
      display: none;
    }
    border: none 0px;
    border-top: 1px solid $gris6;
    border-bottom: 1px solid $gris6;
    padding-top: 5px;
    .field-etiquetes,
    .field-tags,
    .field-territori {
      display: inline-block;
      width: auto;
      @include etiquetes;
      background: none;
    }
  }
  #tags-wrapper {
    padding: 10px 0;
    .field-etiquetes,
    .field-tags,
    .field-tags .field-items .field-item,
    .field-territori {
      display: inline-block;
      width: auto;
      @include etiquetes;
      background: none;
    }
  }
  #block-views-fotogaleria-block,
  #block-views-videogaleria-block {
    //@include zen-grid-item(9,1);
    margin-top: 40px;
    table {
      width:100%;
    }
    .block-title {
      @include graphik-medium(26px,$gris7,30px);
      margin-top:-10px;
    }
    .view-fotogaleria,
    .view-videogaleria {
      .views-field.views-field-title {
        line-height: 20px;
        .field-content {
          line-height: 20px;
          margin-top: 0px;
          a {
            @include graphik-semibold(18px, $gris7,22px); /*variacio disseny*/
          }
        }
      }
    }
    .view-footer {
      @include graphik-semibold(21px,$gris7,24px);
      background-color: $fons;
      padding-top: 5px;
      padding-bottom: 5px;
      text-align: center;
      p {
        margin: 0px;
      }
    }
  }
}


/**
 * Vista de fotogaleries
 * /fotogaleria
 */
body.section-fotogaleria.page-views {
  /**
   * view del peu d'altres fotogaleries 
   */
  #block-views-fotogaleria-block {
    //@include block-totes-fotogaleries();
  }
}

.page-videogaleria.page-views,
.page-fotogaleria.page-views {
  #main #content {
    margin-top: 0px;
  }
}
