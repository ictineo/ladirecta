@import "publico";
@import "graphic";

/*COLORS VAR
  FONTS MIXINS
  SHADOWS
  BOTONES & LOGO
*/

/*COLORS*/
$gris2:#767676;
$gris3:#777777; 
$gris4:#9A9A9A;
$gris5:#939393;
$gris6:#D3D3D3; /*fil d'ariadna*/
$gris7:#5E5E5E; /*inicia sessio*/
$gris8:#535353; //gris titol pagina -- Jude
$gris9: #f0f0f0;
$gris10: #f7f7f7;
$vermell:#C13432;
$blanc: #FFFFFF;
$negre:#030303;
$fons: #e9e9e9; /*gris de fons*/
$negre2:#444444; /*Franja portada pendent confirmacio*/
$fons_rosa: #f7e8f6;
$fons_form: #f7e8f6; //No especificat, cal canviar-lo
$form: #BABABA; //color text form
/** 
 * notes quiron, ordre de grisos
 * blanc -> 9 -> 6 -> 4 -> 5 -> 2 = 3 -> 7 -> 8 -> negre -> negre2
 */


/*MIXINS FONTS*/

@mixin graphik-regular($font-size,$color,$line-height){
  font-family:'graphik_regularregular';
  font-size:$font-size;
  color:$color;
  line-height:$line-height;
  font-weight: normal;
}
@mixin graphik-medium($font-size,$color,$line-height){
  font-family:'graphikmedium';
  font-size:$font-size;
  color:$color;
  line-height:$line-height;
  font-weight: normal;
}
@mixin graphik-semibold($font-size,$color,$line-height){
  font-family:'graphiksemibold';
  font-size:$font-size;
  color:$color;
  line-height:$line-height;
}
@mixin graphik-bold($font-size,$color,$line-height){
  font-family:'graphikbold';
  font-size:$font-size;
  color:$color;
  line-height:$line-height;
}
@mixin graphik-super($font-size,$color,$line-height){
  font-family:'graphiksuper';
  font-size:$font-size;
  color:$color;
  line-height:$line-height;
}
@mixin public-bold($font-size,$color,$line-height){
  font-family:'publico_textsemibold';
  font-size:$font-size;
  color:$color;
  line-height:$line-height;
}
@mixin public-roman($font-size,$color,$line-height){
  font-family:'publico_textroman';
  font-size:$font-size;
  color:$color;
  line-height:$line-height;
}


/*SHADOWS*/

@mixin header-shadow {
  -webkit-box-shadow: 0px 10px 11px -11px rgba(233,233,233,1);
  -moz-box-shadow: 0px 10px 11px -11px rgba(233,233,233,1);
  box-shadow: 0px 10px 11px -11px rgba(233,233,233,1);
}

@mixin menu-shadow {
 -webkit-box-shadow: inset 0px 15px 12px -12px rgba(233,233,233,1);
 -moz-box-shadow: inset 0px 15px 12px -12px rgba(233,233,233,1);
  box-shadow: inset 0px 15px 12px -12px rgba(233,233,233,1);
}
@mixin line-gradient {
 background: radial-gradient(ellipse at center, rgba(0,0,0,0.1) 0%,rgba(0,0,0,0) 75%);
}
@mixin box-shadow {
 -webkit-box-shadow: inset 0px 15px 12px 0px rgba(233,233,233,1);
 -moz-box-shadow: inset 0px 15px 12px 0px rgba(233,233,233,1);
  box-shadow: inset 0px 15px 12px 0px rgba(233,233,233,1); 
}
@mixin login-shadow {
-webkit-box-shadow: inset 0px 0px 2px 2px rgba(227,227,227,1);
-moz-box-shadow: inset 0px 0px 2px 2px rgba(227,227,227,1);
box-shadow: inset 0px 0px 2px 2px rgba(227,227,227,1);
}
/*shadows*/
@mixin header-shadow {
  -webkit-box-shadow: 0px 10px 11px -11px rgba(233,233,233,1);
  -moz-box-shadow: 0px 10px 11px -11px rgba(233,233,233,1);
  box-shadow: 0px 10px 11px -11px rgba(233,233,233,1);
}
@mixin menu-shadow {
 -webkit-box-shadow: inset 0px 15px 12px -12px rgba(233,233,233,1);
 -moz-box-shadow: inset 0px 15px 12px -12px rgba(233,233,233,1);
  box-shadow: inset 0px 15px 12px -12px rgba(233,233,233,1); 
}
@mixin line-gradient {
    background: radial-gradient(ellipse at center, rgba(0,0,0,0.1) 0%,rgba(0,0,0,0) 75%);
}
@mixin box-shadow {
 -webkit-box-shadow: inset 0px 15px 12px 0px rgba(233,233,233,1);
 -moz-box-shadow: inset 0px 15px 12px 0px rgba(233,233,233,1);
